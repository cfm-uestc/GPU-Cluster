@model IEnumerable<GPUCluster.Shared.Models.Workload.Image>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tag)
            </th>
            <th>
                
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastModifiedTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tag)
            </td>
            <td>
                Loaded in XXX conainters
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModifiedTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VolumeID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VolumeID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VolumeID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
